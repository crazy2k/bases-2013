\subsection{Restricciones}
\begin{itemize}
    \item
            \textbf{Jugador}.\textsl{nro\_pasaporte} debe estar en 
            \textbf{Integrante}.\textsl{nro\_pasaporte}

    \item
            \textbf{Cuerpo\_Tecnico}.\textsl{nro\_pasaporte} 
            debe estar en \textbf{Integrante}.\textsl{nro\_pasaporte}
            
    \item
            \textbf{Integrante}.\textsl{id\_equipo} no puede ser nulo y debe estar en 
            \textbf{Equipo\_Nacional}.\textsl{id\_equipo}
    
    \item
            \textbf{Equipo\_Nacional}.\textsl{id\_equipo} debe estar en 
            \textbf{Integrante}.\textsl{id\_equipo}
    
    \item
            \textbf{Equipo\_Nacional}.\textsl{id\_estadio\_asignado} no puede ser 
            nulo y debe estar en \textbf{Estadio}.\textsl{id\_estadio}
            
    \item
            \textbf{Estadio}.\textsl{id\_estadio} debe estar en 
            \textbf{Equipo\_Nacional}.\textsl{id\_estadio\_asignado}
    
    \item
            \textbf{Equipo\_Nacional}.\textsl{id\_pais\_representado} no puede ser 
            nulo y debe estar en \textbf{Pais}.\textsl{id\_pais}
            
    \item
            \textbf{Pais}.\textbf{id\_pais} puede no estar en 
            \textbf{Equipo\_Nacional}.\textsl{id\_pais\_representado}
            
    \item
            \textbf{Pais}.\textsl{id\_pais} no puede estar más de una vez en 
            \textbf{Equipo\_Nacional}.\textsl{id\_pais\_representado}
            
    \item
            \textbf{Arbitro}.\textsl{id\_pais\_nacionalidad} no puede ser nulo y 
            debe estar en \textbf{Pais}.\textsl{id\_pais}
            
    \item
            \textbf{Pais}.\textsl{id\_pais} puede no estar en 
            \textbf{Arbitro}.\textsl{id\_pais\_nacionalidad}
            
    \item
            \textbf{Pais}.\textsl{id\_pais} no puede estar más de una vez en 
            \textbf{Arbitro}.\textsl{id\_pais\_nacionalidad}
            
    \item
            \textbf{Partido}.\textsl{id\_estadio} no puede ser nulo y debe estar en 
            \textbf{Estadio}.\textsl{id\_estadio}
            
    \item        
            \textbf{Equipo\_Nacional}.\textsl{id\_equipo} debe estar en 
            \textbf{Juega}.\textsl{id\_equipo}
            
    \item
            \textbf{Partido}.\textsl{id\_partido} debe estar en \textbf{Juega}.\textsl{id\_partido}
            
    \item
            \textbf{Juega}.\textsl{id\_partido} debe estar en \textbf{Partido}.\textsl{id\_partido}
            
    \item
            \textbf{Juega}.\textsl{id\_equipo} debe estar en 
            \textbf{Equipo\_Nacional}.\textsl{id\_equipo}
            
    \item
            \textbf{Arbitro}.\textsl{id\_partido} debe estar en \textbf{Arbitra}.\textsl{id\_partido}
            
    \item
            \textbf{Partido}.\textsl{id\_partido} debe estar en \textbf{Arbitra}.\textsl{id\_partido}
            
    \item
            \textbf{Arbitra}.\textsl{id\_partido} debe estar en \textbf{Arbitro}.\textsl{id\_partido}
            
    \item
            \textbf{Arbitra}.\textsl{id\_partido} debe estar en \textbf{Partido}.\textsl{id\_partido}
            
    \item
            \textbf{Juego}.\textsl{id\_juego} debe estar en 
            \textbf{Imparte\_Sancion\_En}.\textsl{id\_juego}
            
    \item
            \textbf{Arbitro}.\textsl{id\_partido} debe estar en 
            \textbf{Imparte\_Sancion\_En}.\textsl{id\_partido}
            
    \item
            \textbf{Imparte\_Sancion\_En}.\textsl{id\_partido} debe estar en 
            \textbf{Arbitro}.\textsl{id\_partido}
            
    \item
            \textbf{Imparte\_Sancion\_En}.\textsl{id\_juego} debe estar en 
            \textbf{Juego}.\textsl{id\_juego}
            
    \item
            \textbf{Partido}.id\_fase no puede ser nulo y debe estar en 
            \textbf{Fase}.id\_fase
            
    \item
            \textbf{Juego}.\textsl{id\_partido} no puede ser nulo y debe estar en 
            \textbf{Partido}.\textsl{id\_partido}
            
    \item
            \textbf{Juego}.\textsl{id\_juego} puede no estar en 
            \textbf{Imparte\_Sansion\_En}.\textsl{id\_juego}
            
    \item
            \textbf{Partido}.\textsl{id\_fase} no puede ser nulo y debe estar en \textbf{Fase}.\textsl{id\_fase}
            
    \item
            \textbf{Juego}.\textsl{nro\_pasaporte\_jugador} no puede ser nulo y debe estar en \textbf{Jugador}.\textsl{nro\_pasaporte}
            
    \item
            \textbf{Resultado\_Cuarto}.\textsl{id\_partido} debe estar en \textbf{Partido}.\textsl{id\_partido}
\end{itemize}

\subsection{Restricciones}
\begin{itemize}
    \item
            Todos los estadios est\'an asignados a un equipo nacional para que concentren.
\end{itemize}
